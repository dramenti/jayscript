PRINT "Now for nested while:"
SET i = 1
SET dim = 3
WHILE i <= dim
    SET j = 1
    WHILE j <= dim
        PRINT j #should print 1,2,3,1,2,3,1,2,3
        SET j = j+1
    END
    SET i = i+1
END